{
  "name": "cloudku-uploader",
  "version": "3.0.0",
  "description": "Blazing-fast, zero-dependency uploader for CloudKu. Supports auto-conversion, chunked uploads, and TypeScript. Easily upload images, videos, audio, and documents via Node.js.",
  "license": "MIT",

  "author": {
    "name": "Alfi Dev",
    "email": "support@cloudkuimages.com",
    "url": "https://cloudkuimages.guru"
  },

  "homepage": "https://cloudkuimages.guru",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudkuimages/cloudku-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/cloudkuimages/cloudku-uploader/issues",
    "email": "support@cloudkuimages.com"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/cloudkuimages"
  },

  "type": "module",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",

  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },

  "engines": {
    "node": ">=20.0.0"
  },

  "publishConfig": {
    "access": "public"
  },

  "scripts": {},

  "keywords": [
    "cloudku",
    "cloudkuimages",
    "uploader",
    "file-upload",
    "cloud-storage",
    "image-upload",
    "video-upload",
    "audio-upload",
    "document-upload",
    "chunked-upload",
    "auto-convert",
    "typescript",
    "nodejs",
    "esm",
    "cjs",
    "modern",
    "zero-dependencies",
    "fast-upload",
    "progress-tracking",
    "multi-format",
    "cross-platform"
  ]
}
